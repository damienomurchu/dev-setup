{
  "files.autoSave": "afterDelay",
  "explorer.confirmDelete": false,
  "editor.tabSize": 2,
  "editor.dragAndDrop": false,
  "editor.hover.enabled": false,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.acceptSuggestionOnEnter": "off",
  "files.hotExit": "onExitAndWindowClose",
  "workbench.enableExperiments": false,
  "workbench.colorTheme": "Solarized Dark",
  "workbench.panel.defaultLocation": "right",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.settings.openDefaultKeybindings": true,
  "window.restoreWindows": "folders",
  "search.showLineNumbers": true,
  "scm.countBadge": "off",
  "scm.defaultViewMode": "tree",
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.copyOnSelection": true,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "emmet.triggerExpansionOnTab": true,
  "html.format.indentInnerHtml": true,
  "debug.node.autoAttach": "on",
  "explorer.confirmDragAndDrop": false,
  "spellright.language": [],
  "cSpell.userWords": [
    "integreatly",
    "rhmi"
  ],
  "workbench.iconTheme": "vscode-icons",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "terminal.integrated.fontFamily": "\"MesloLGM Nerd Font Mono\", 'Cascadia Code'",
  "yaml.format.singleQuote": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[json]": {
    "editor.defaultFormatter": "remimarsal.prettier-now"
  },
  "go.formatTool": "goimports",
  "editor.detectIndentation": false,
  "workbench.startupEditor": "newUntitledFile",
  "python.linting.enabled": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "todo-tree.tree.showScanModeButton": false,
  "browser-preview.startUrl": "https://duckduckgo.com",
  "editor.fontFamily": "'Cascadia Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback', 'FontAwesome'",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.wordWrap": "on",
  "markdown-pdf.headerTemplate": "",
  "markdown-pdf.displayHeaderFooter": false,
  "tabnine.experimentalAutoImports": true,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 1,
  "liveServer.settings.donotShowInfoMsg": true,
  // "window.menuBarVisibility": "hidden",
  "vsicons.dontShowNewVersionMessage": true,
  "vetur.experimental.templateInterpolationService": true,
  "workbench.sideBar.location": "right",
  "editor.multiCursorModifier": "ctrlCmd",
  "security.workspace.trust.untrustedFiles": "newWindow"
}